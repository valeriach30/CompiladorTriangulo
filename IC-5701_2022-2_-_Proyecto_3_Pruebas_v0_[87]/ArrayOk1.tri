let
  var lista : array 5 of Integer ;
  var i : Integer
in ! begin
  ! llenar arreglo lista
  i := 0 ;
  loop while i < 5 do ! begin
    lista [i] := i * 10 ;
    i := i + 1
  end ;
  ! imprimir arreglo lista
  i := 0 ;
  loop while i < 5 do ! begin
    putint(lista [i]) ;
    puteol() ;
    i := i + 1
  end    
end
