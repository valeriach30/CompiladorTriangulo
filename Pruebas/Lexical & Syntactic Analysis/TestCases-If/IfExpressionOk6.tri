! If expression successful test
! if E1 then E2 else E3 where
! where E1 and E3 are if expressions

let
  var a : Integer;
  var b : Integer;
  var x : Integer;
  var y : Integer;
  var bigger : Integer
in
  bigger := if (a > b) then
    if (x < a) then
            a
        else
            x
  else
    if (y > b) then
            y
        else
            b